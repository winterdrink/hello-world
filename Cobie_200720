%     {'survey_score.xlsx'       }
%     {'total_cost_times.xlsx'   }
%     {'wordset_with_rating.xlsx'}

% 뭐해야 하는거지...?

if s == 12
  for i = 1:10
       [num{i},txt{i},raw{i}] = xlsread(fullfile(subjectdir{1}, filenames_sub{i+2})); 
  end
elseif s ~= 12
       warning('information incorrect') % warning : 끝까지 돌아감, error : 끝까지 안 돌아감
end

% ratings for each exam, to read a excel xls file, just xlsread = num, [num, txt, raw], [~, txt, raw], [~, ~, raw], () 썼으니 cell, raw = num or txt everything


str2num(txt{10}{160,3})+1           % string > double
num2str(str2num(txt{10}{160,3})+1)  % double > string

% for Mac
y = readtable(fullfile(subjectdir{1}, filenames_sub{8}),'ReadVariableNames',false); % readtable : 파일에서 열 방향 데이터를 읽어 테이블을 만듭니다.
age = str2num(string(y{1,1}))
num2str(age)
% y = num2str(y) -> character, y = string(y) -> string

% 

% 'ReadVariablenames', false 지우면
% 경고: 테이블에 대한 변수 이름을 생성하기 전에 파일의 열 제목이 유효한 MATLAB 식별자가 되도록 수정되었습니다. 원래 열 제목은
% VariableDescriptions 속성에 저장되어 있습니다.
% 원래 열 제목을 테이블 변수 이름으로 사용하려면 'PreserveVariableNames'를 true로 설정하십시오. 
% false로 하면 앞에 내가 변수 이름 새로 설정하는거고, true로 하면 그대로 받는건가?
% 근데 얘가 matlab에서는 변수로 안나오는데 왜 설정하는거지?

clear num txt raw


%%
basedir = '/Users/dongjupark/Dropbox/summerintern_2020/scripts/Week1';
dataname = fullfile(basedir, 'age_sex_education_n50_dongju.mat');
load(dataname) % x = load(dataname)

subject_num = 50;
subject_num = numel(filenames);

% preallocation % 미리 해두면 부담이 적다!
age = zeros(subject_num,1);
sex = cell(subject_num,1);
edu = cell(subject_num,1);

% preallocation
sex_d2 = zeros(numel(sex),1);
edu_d2 = zeros(numel(edu),1);
edu_category = {'고졸', '대학교재학', '대학원재학', '대학원졸업'};

% commend + i : auto 들여쓰

for i = 1:numel(sex)
    subjectdir = fullfile(datdir, filenames{i});
    filelist_sub = dir(string(subjectdir))';
    filenames_sub = {filelist_sub.name}' ;
    y = readtable(fullfile(subjectdir, filenames_sub{8}),'ReadVariableNames',false);
    sex{i,1} = string(y{2,1})
    edu{i,1} = string(y{3,1})
    % sex 
    if strcmp(sex{i}, 'Female')
        sex_d2(i,1) = 1;
    elseif strcmp(sex{i}, 'Male')
        sex_d2(i,1) = 2;
    else
        warning(sprintf('Dose the subject-%02d have 3rd sex?.',i)) 
    end
    sex_double = sex_d2
  
    % edu
    edu{i} = string(y{3,1});
    if strcmp(edu{i}, edu_category{1})
        edu_d2(i,1) = 1;
    elseif strcmp(edu{i}, edu_category{2})
        edu_d2(i,1) = 2;
    elseif strcmp(edu{i}, edu_category{3})
        edu_d2(i,1) = 3;
    elseif strcmp(edu{i}, edu_category{4})
        edu_d2(i,1) = 4;
    end
    edu_double = edu_d2
    end



%% histogram
% age
%  

for i = 1:numel(age)
    subjectdir = fullfile(datdir, filenames{i});
    filelist_sub = dir(string(subjectdir))'; 
    filenames_sub = {filelist_sub.name}' ;
    y = readtable(fullfile(subjectdir, filenames_sub{8}),'ReadVariableNames',false);
    age(i) = string(y{1,1});
end
    
      
    
    s = size(filenames_sub,1);

% age = data.age; % ?
% age cell일 때 314-316
% age = string(age)
% age = str2double(age) % 미친 str2num은 셀형 배열 또는 비 스칼라 string형 배열을 변환하지 않으며, + 연산자와 - 연산자 앞뒤의 공백을 무시하지 않습니다

figure;
counts = 10; % number of bars
h = histogram(age, 'NumBins', counts); % h -> show you all the handles, features, Numbins -> # of bars
h.BinLimits = [15 40]; % start and end of bars in x asis
% h = histogram(age, 'NumBins', counts, 'Binlimits', [15 40]); all in one!,
% 여기서는 Numbins, 즉 bar 갯수가 10개, 위에서처럼 순서대로 하면 20개
h.Parent.YLim = [0 20] % y axis change!
title('distribution of age','FontSize',15)
xlabel('age','FontSize',15)
ylabel('counts','FontSize',15)
text(30, 10, sprintf('mean = %0.2f',mean(age)),'FontSize',15) % percentage with 2 under number after .
text(30, 9, sprintf('std = %0.2f',std(age)),'FontSize',15) % 30&9 = location
fullfile(savedir, 'distribution of age.fig')
saveas(gcf,'dj_distribution of age.pdf')

set(gcf, 'color', 'w') % not get, set first!
get(gcf) % gcf = the very first figure that is open now, background of figure, out of axises
set(gcf, 'color', 'w', 'position', [680 385 560 420]) 
% figure 비율 조절한 다음 gcf로 position confirm, [전체 화면 왼쪽 아래 시작점의 x 좌표, 왼쪽 아래 y 좌표, 가로 창의 크기, 세로 창의 크기]에 넣으면 끝
% gcf, 'color', 'w' : white, 'color', 'r' : red, 'color', 'b' : blue etc.

% get(gcf, 'color')
% get(gcf, 'position')

% sprintf
sprintf('mean = %0.2f',mean(age))

% sex
% sex_d = data.sex_double;
% x = histcounts(sex_d);
x = histcounts(sex_double) % histcounts : counts number of each variables 
figure;
c = categorical({'Female','Male'});
b = bar(c, x);
b.FaceColor = 'flat'; % changing colors of each bars is possible with this 'flat'
b.CData(1,:) = [0 .3 .3];
b.CData(2,:) = [.3 0 .3];
b.BarWidth = .5;
text(1, x(1)+1, num2str(x(1)))
text(2, x(2)+1, num2str(x(2)))
text(1.3, 10, sprintf('mean = %0.2f',mean(sex_double)),'FontSize',10) 
text(1.3, 11, sprintf('std = %0.2f',std(sex_double)),'FontSize',10)
title('Female&male','FontSize',15)
fullfile(savedir, 'dj_Female&m.fig')
saveas(gcf,'dj_Female&m.png')



% education
y = histcounts(education_double)
figure;
d = categorical({'고졸', '대학교재학', '대학원재학', '대학원졸업'});
bb = bar(d, y);
bb.FaceColor = 'flat';
bb.CData(2,:) = [0 .5 .5];
bb.CData(3,:) = [.8 0 .3];
bb.CData(4,:) = [0 .9 0];
bb.CData(1,:) = [.2 0 .7];
bb.BarWidth = .3;
text(1, y(1)+1, num2str(y(1)))
text(2, y(2)+1, num2str(y(2)))
text(3, y(3)+1, num2str(y(3)))
text(4, y(4)+1, num2str(y(4)))
text(1, 25, sprintf('mean = %0.2f',mean(education_double)),'FontSize',20) 
text(1, 22, sprintf('std = %0.2f',std(education_double)),'FontSize',20)
title('Education','FontSize',15)
fullfile(savedir, 'dj_Education.fig')
saveas(gcf,'dj_Education.png')


